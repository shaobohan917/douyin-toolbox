{
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "Douyin Toolbox",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/history/history",
      "style": {
        "navigationBarTitleText": "History",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/settings/settings",
      "style": {
        "navigationBarTitleText": "Settings"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "Douyin Toolbox",
    "navigationBarBackgroundColor": "#FF2E4D",
    "backgroundColor": "#F8F8F8",
    "backgroundTextStyle": "dark",
    "app-plus": {
      "background": "#EFEFF4"
    }
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#FF2E4D",
    "borderStyle": "black",
    "backgroundColor": "#FFFFFF",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "Home",
        "iconPath": "static/images/home.png",
        "selectedIconPath": "static/images/home-active.png"
      },
      {
        "pagePath": "pages/history/history",
        "text": "History",
        "iconPath": "static/images/history.png",
        "selectedIconPath": "static/images/history-active.png"
      },
      {
        "pagePath": "pages/settings/settings",
        "text": "Settings",
        "iconPath": "static/images/settings.png",
        "selectedIconPath": "static/images/settings-active.png"
      }
    ]
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue"
    }
  }
}
